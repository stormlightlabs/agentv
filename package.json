{
  "name": "agent-viz",
  "version": "0.1.0",
  "description": "Agent session visualization and analysis tool",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter agent-viz-gui dev",
    "build": "pnpm --filter agent-viz-gui build",
    "check": "pnpm --filter agent-viz-gui check",
    "tauri": "pnpm --filter agent-viz-gui tauri",
    "cli:build": "cargo build --bin agent-viz",
    "cli:run": "cargo run --bin agent-viz",
    "test": "cargo test",
    "lint": "cargo clippy",
    "format": "cargo fmt"
  },
  "devDependencies": {
    "prettier": "^3.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tauri-apps/cli",
      "esbuild"
    ]
  }
}
